{
  "name": "chili-playground",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run lint && npm run copyfiles",
    "serve": "node dist/server.js",
    "copyfiles": "ncp src/resources dist/resources --stopOnErr",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\"  \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "engines": {
    "npm": "6.14.2",
    "node": "13.8.0",
    "yarn": "1.17.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "nodemon": "^2.0.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@google-cloud/vision": "^1.10.0",
    "@types/bluebird": "^3.5.30",
    "@types/express": "^4.17.3",
    "@types/formidable": "^1.0.31",
    "@types/node": "^13.9.2",
    "@types/sequelize": "^4.28.8",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/parser": "^2.23.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "formidable": "^1.2.2",
    "hbs": "^4.1.0",
    "helmet": "^3.21.3",
    "lodash": "^4.17.19",
    "minimist": "^1.2.5",
    "multer": "^1.4.2",
    "ncp": "^2.0.0",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "prettify": "^0.1.7",
    "sequelize": "^5.21.5",
    "ts-node": "^8.6.2",
    "yarn.lock": "^0.0.1-security"
  }
}
